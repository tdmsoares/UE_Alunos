Function ValidarNome(ByVal Texto As String) As String
'
'Needs String_Helper Module to work
If (Not String_Helper.isVálidoNome(Texto)) Then
    Texto = String_Helper.RemoverDiacríticos(Texto)
    Texto = String_Helper.RemoverExcessoEspaçosEntrePalavras(Texto)
    Texto = Trim(Texto)
End If
ValidarNome = Texto
End Function

