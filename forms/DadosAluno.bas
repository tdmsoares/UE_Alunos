Private Sub Form_Timer()
'
On Error GoTo Errado
    '
    marcadorTempo = marcadorTempo + 1
    '
    'Alerta Liminar
    If (FlagLiminar = True) Then
        If ((marcadorTempo Mod 2) = 0) Then
            Toggle_Alerta_Liminar True
        Else
            Toggle_Alerta_Liminar False
        End If
    End If
    '
    'Verifica Alerta na Retirada
    Dim AlertaRetirada
    AlertaRetirada = Me.recordset!Alerta_Retirada
    '
    If (AlertaRetirada = "") Or (AlertaRetirada = "Não Informado") Then
        Me.Status.SetFocus
        Me.Alerta_Retirada.Visible = False
    ElseIf (AlertaRetirada = "Alerta") Then
        Me.Alerta_Retirada.Caption = "ALERTA"
        Me.Alerta_Retirada.Visible = True
        Me.Alerta_Retirada.BackStyle = 1
        Me.Alerta_Retirada.BackColor = RGB(153, 255, 51)
        Me.Alerta_Retirada.ForeColor = RGB(0, 0, 0)
    ElseIf (AlertaRetirada = "Impedimento") Then
        Me.Alerta_Retirada.Caption = "IMPEDIMENTO"
        Me.Alerta_Retirada.Visible = True
        '
        If ((marcadorTempo Mod 2) = 0) Then
            Toggle_Impedimento_Retirada (True)
        Else:
            Toggle_Impedimento_Retirada (False)
        End If
    End If
    '
    'Define o tempo do Timer
    If (marcadorTempo >= 10) Then
        marcadorTempo = 1
        Me.TimerInterval = 1500
    End If
    
Errado:
If (Err.Number = 3021) Then
    Resume Next
End If
End Sub

