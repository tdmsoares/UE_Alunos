Sub LockAnotações()
    '
    'Bloqueia Edições no Subformulário Anotações
    Me.LAnotações.Value = True
    Me.UAnotações.Value = False
    Me.Anotações_Subformulário.Locked = True
    Me.Anotações_Subformulário.SpecialEffect = 3
    Me.Anotações_Subformulário.Form.AllowDeletions = False
End Sub

Sub UnlockAnotações()
    '
    'Desbloqueia Edições no Subformulário Anotações
    Me.LAnotações.Value = False
    Me.UAnotações.Value = True
    Me.Anotações_Subformulário.Locked = False
    Me.Anotações_Subformulário.SpecialEffect = 2
    Me.Anotações_Subformulário.Form.AllowDeletions = True
End Sub

Private Sub UAnotações_Click()
    UnlockAnotações
End Sub

Sub Toggle_Alerta_Liminar(Flag As Boolean)
    Me.Liminar.Visible = True
    If (Flag = True) Then
        Me.Liminar.BackStyle = 1
    Else:
        Me.Liminar.BackStyle = 0
    End If
End Sub

Sub Toggle_Impedimento_Retirada(Flag As Boolean)
    If (Flag = True) Then
        Me.Alerta_Retirada.BackStyle = 1
        Me.Alerta_Retirada.BackColor = RGB(255, 0, 0)
        Me.Alerta_Retirada.ForeColor = RGB(255, 255, 255)
        Me.Caixa106.BorderColor = RGB(114, 163, 118)
    Else:
        Me.Alerta_Retirada.BackStyle = 0
        Me.Alerta_Retirada.ForeColor = RGB(0, 0, 0)
        Me.Caixa106.BorderColor = RGB(255, 255, 0)
    End If
End Sub
